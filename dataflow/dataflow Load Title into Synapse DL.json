{
	"name": "dataflow Load Title into Synapse DL",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_adls_nycpayroll_TitleMaster",
						"type": "DatasetReference"
					},
					"name": "source"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_adls_nycpayroll_synapse",
						"type": "DatasetReference"
					},
					"name": "sink"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source",
				"source sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['NYC_Payroll_TITLE_MD.csv'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          TitleCode,",
				"          TitleDescription",
				"     ),",
				"     partitionBy('hash', 1)) ~> sink"
			]
		}
	}
}